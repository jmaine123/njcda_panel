<h1>Pamela Panel User's</h1>
<table class="index">
  <tr class='list_header'>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Admin</th>
  </tr>
  <% @users.each do |i|%>
  <tr>
    <td> <%=i.first_name%></td>
    <td><%=i.last_name%></td>
    <td><%=i.email%></td>
    <td><%=i.admin%></td>
  </tr>
  <%end%>
</table>
