<%= form_for([@course, @cohort, @student]) do |f|%>

  <%= f.text_field :first_name, placeholder: 'Enter First Name', class:'name' %>
  <%= f.text_field :last_name, placeholder: 'Enter Last Name' class: 'name'%><br>
  <%= f.email_field :email, placeholder: 'Enter email', class:'email' %><br>
  <%= f.telephone_field :phone_number, placeholder: 'phone number'%>
  <%= f.number_field :age, placeholder: 'age', class:'age' %><br><br>
  <%= label_tag("Highest completed education") %><br><br>
  <%= f.radio_button(:education, 'High School') %> High School<br>
  <%= f.radio_button(:education, 'College')%> College<br>
  <%= f.radio_button(:education, 'Masters') %> Masters<br>
  <%= f.radio_button(:education, 'PHD')%>PHD<br><br>

  <%= f.submit 'Register'%>

<% end %>
